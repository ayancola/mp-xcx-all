.news_detail .detil_con{width: 100%;box-sizing: border-box;overflow: hidden;line-height: 1.8;font-size: 28rpx;}

/*photo*/
.photo .listItem{width: 48%;float: left;margin: 30rpx 1%;}
.photo .listItem .listImg{width: 100%;height: 256rpx;padding: 20rpx;border: 1px solid #f5f5f5;border-radius: 10rpx;box-sizing: border-box;}
.photo .listItem .listImg image{width: 100%;height: 100%;}
.photo .listItem .listTitle{width: 100%;height: 30rpx;line-height: 30rpx;color: #999999;text-align: center;font-size: 32rpx;margin-top: 20rpx;}

/*search*/
.search form{ background:  #fff; display: block; border-radius: 999px; padding: 12rpx 40rpx; margin-top: 40rpx;}
.search form input{line-height: 1; width: calc(100% - 32rpx); padding-right: 30rpx; box-sizing: border-box;}
.search form button{width: 32rpx; height: 32rpx;background: transparent; padding: 0;}
.search form button::after{border: none; }
.search .list-img{ height: 268rpx;  overflow:hidden;border-radius: 10rpx;}
.search .list-img image{height:100%;}
.search .list{width: 47.5%;margin-bottom: 30rpx;}
.search .view-text{padding: 20rpx;text-align: center;}
.search .page-T { background-color: #666;}

